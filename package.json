{
  "name": "mindsupport-backend",
  "version": "1.0.0",
  "description": "Backend API for MindSupport - Mobile-first mental health support platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "npm run build",
    "postbuild": "echo 'Build completed successfully. Checking dist directory...' && ls -la dist/",
    "prebuild": "rm -rf dist",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed:keywords": "ts-node scripts/seedKeywords.ts",
    "vercel-build": "tsc"
  },
  "keywords": ["mental-health", "chat", "moderation", "stream-chat"],
  "author": "Rohanvish4",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "stream-chat": "^8.14.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.11",
    "zod": "^3.22.4",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "@sentry/node": "^7.91.0",
    "nanoid": "^3.3.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}